    "jquery": "^3.7.1",
    "pinia": "^2.1.6",
    "popper.js": "^1.16.1",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "vue-video-player": "^6.0.0",
    "vue3-video-play": "^1.3.1-beta.6",
    "xgplayer": "^3.0.9"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.17.17",
    "@vitejs/plugin-vue": "^4.3.4",
